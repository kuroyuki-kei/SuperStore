<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>com.superstore.bean</display-name>
  <welcome-file-list>
    <welcome-file>/home.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>productList</servlet-name>
    <servlet-class>com.superstore.servlets.ProductListServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>productList</servlet-name>
    <url-pattern>/productList</url-pattern>
  </servlet-mapping>
  
    <servlet>
    <servlet-name>Account</servlet-name>
    <servlet-class>com.superstore.servlets.AccountServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>Account</servlet-name>
    <url-pattern>/Account</url-pattern>
  </servlet-mapping>
  
    <servlet>
    <servlet-name>UsernameQuery</servlet-name>
    <servlet-class>com.superstore.servlets.UsernameQuery</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>UsernameQuery</servlet-name>
    <url-pattern>/UsernameQuery</url-pattern>
  </servlet-mapping>
  
</web-app>
